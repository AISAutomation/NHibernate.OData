<log4net debug="false">
  <appender name="A1" type="log4net.Appender.ConsoleAppender">
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%-4r [%t] %-5p %c %x - %m%n" />
    </layout>
  </appender>
  <root>
    <level value="DEBUG" />
    <appender-ref ref="A1" />
  </root>
  <logger name="NHibernate">
    <level value="WARN" />
  </logger>
</log4net>
